	 rdfs:label="workcontext:currentThread">
	<rdfs:range rdf:resource="&workcontext;MediumTermContextThread"/>
	<rdfs:domain rdf:resource="&workcontext;UserState"/>
</rdf:Property>
<rdf:Property rdf:about="&workcontext;delimitation"
	 a:maxCardinality="1"
	 a:minCardinality="1"
	 rdfs:label="workcontext:delimitation"/>
