	<rdfs:subClassOf rdf:resource="&ncal;UnionOfTimezoneObservanceEventFreebusyTimezoneTodo"/>
	<rdfs:subClassOf rdf:resource="&ncal;UnionOfTimezoneObservanceEventJournalTimezoneTodo"/>
</rdfs:Class>
<rdfs:Class rdf:about="&ncal;Todo"
	 rdfs:label="ncal:Todo">
