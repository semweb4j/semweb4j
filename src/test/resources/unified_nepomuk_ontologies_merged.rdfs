	 rdfs:comment="Lists the elements in this thread."
	 rdfs:label="workcontext:containsElement">
	<rdfs:domain rdf:resource="&workcontext;ContextRepresentation"/>
	<rdfs:range rdf:resource="&workcontext;ContextualElement"/>
	<a:inverseProperty rdf:resource="&workcontext;belongsToContainer"/>
