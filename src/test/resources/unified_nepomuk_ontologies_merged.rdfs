</rdf:Property>
<rdf:Property rdf:about="&workcontext;belongsToContainer"
	 a:maxCardinality="1"
	 a:minCardinality="1"
	 rdfs:label="workcontext:belongsToContainer">
	<rdfs:comment>A contextual element belongs to exactly one Context Thread and hence contributes to this contex thread.</rdfs:comment>
	<rdfs:range rdf:resource="&workcontext;ContextRepresentation"/>
	<rdfs:domain rdf:resource="&workcontext;ContextualElement"/>
	<a:inverseProperty rdf:resource="&workcontext;containsElement"/>
</rdf:Property>
<rdf:Property rdf:about="&workcontext;belongsToGroup"
	 rdfs:comment="A contextual element can belong to a group."
	 rdfs:label="workcontext:belongsToGroup">
	<rdfs:domain rdf:resource="&workcontext;ContextualElement"/>
	<rdfs:range rdf:resource="&workcontext;ContextualElementGrouping"/>
</rdf:Property>
