<rdf:Property rdf:about="&workcontext;containsSpecification"
	 rdfs:comment="Define statem"
	 rdfs:label="workcontext:containsSpecification">
	<rdfs:range rdf:resource="&workcontext;ContextualWorldView"/>
</rdf:Property>
<rdf:Property rdf:about="&workcontext;currentAttentionValue"
	 a:maxCardinality="1"
	 a:maxValue="1.0"
	 a:minCardinality="1"
	 a:minValue="0.0"
	 a:range="float"
	 rdfs:label="workcontext:currentAttentionValue">
	<rdfs:comment>The current attention value of the user towards this element. Expresses how much attention the user has given to the connected resource within the current context thread. This value is subject to change continually, it is dynamically computed by the ContextBoiler.</rdfs:comment>
	<rdfs:domain rdf:resource="&workcontext;ContextualElement"/>
	<rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
