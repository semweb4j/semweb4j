<?xml version="1.0"?>
<project>
	<parent>
		<artifactId>semweb4j-parent</artifactId>
		<groupId>org.semweb4j</groupId>
		<version>4.3-SNAPSHOT</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.semweb4j</groupId>
	<artifactId>rdfreactor-runtime</artifactId>
	<name>RDFReactor runtime library</name>
	<description>
		RDFReactor creates domain-specific, type-safe Java classes which
		act as a view over domain-free, loosely typed RDF instances.

		This library needs to be shipped togehter with generated
		classes.
	</description>
	<url>http://semweb4.org/rdfreactor</url>
	<inceptionYear>2005</inceptionYear>
	<dependencies>
		<dependency>
			<groupId>org.semweb4j</groupId>
			<artifactId>rdf2go</artifactId>
			<version>4.3-SNAPSHOT</version>
		</dependency>
		<dependency>
			<!--  needed for datatype handling of xsd datetime -->
			<groupId>net.sourceforge.jena</groupId>
			<artifactId>jena</artifactId>
			<version>2.4</version>
		</dependency>
		<dependency>
			<groupId>org.semweb4j</groupId>
			<artifactId>rdf2go-jena24</artifactId>
			<version>4.3-SNAPSHOT</version>
			<scope>test</scope>
		</dependency>
	</dependencies>
</project>