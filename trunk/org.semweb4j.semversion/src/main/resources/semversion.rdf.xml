<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
	xmlns:semversion="http://purl.org/net/semversion#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:owl="http://www.w3.org/2002/07/owl#">

<rdf:Description rdf:about="http://purl.org/net/semversion#VersionedItem">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:comment>valid time, transaction time and core metadata</rdfs:comment>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#VersionedModel">
	<rdfs:subClassOf rdf:resource="http://purl.org/net/semversion#VersionedItem"/>
	<rdfs:comment>A tree of Versions</rdfs:comment>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#Version">
	<rdfs:subClassOf rdf:resource="http://purl.org/net/semversion#VersionedItem"/>
	<rdfs:comment>A set of triples (content TripleSet) with a set of metadata</rdfs:comment>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#RDFModel">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:comment>The URI of this resource denotes a NamedGraph in the SemVersion store - the named graph does not need to contain any data</rdfs:comment>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#User">
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
	<rdfs:comment>A SemVersion User</rdfs:comment>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasVersionedModel">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
	<rdfs:range rdf:resource="http://purl.org/net/semversion#VersionedModel"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasTag">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#VersionedItem"/>
	<rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasAuthor">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#VersionedItem"/>
	<rdfs:range rdf:resource="http://purl.org/net/semversion#User"/>
	<owl:maxCardinality>1</owl:maxCardinality>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasCreationTime">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#VersionedItem"/>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
	<owl:cardinality>1</owl:cardinality>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasDeletionTime">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#VersionedItem"/>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
	<owl:maxCardinality>1</owl:maxCardinality>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasUserdefinedMetadata">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#VersionedItem"/>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
	<owl:maxCardinality>1</owl:maxCardinality>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasProvenance">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#VersionedItem"/>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
	<owl:maxCardinality>1</owl:maxCardinality>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasRoot">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:comment>has only one root</rdfs:comment>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#VersionedModel"/>
	<rdfs:range rdf:resource="http://purl.org/net/semversion#Version"/>
	<owl:maxCardinality>1</owl:maxCardinality>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasVersion">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:comment>exhaustive list of all versions</rdfs:comment>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#VersionedModel"/>
	<rdfs:range rdf:resource="http://purl.org/net/semversion#Version"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasContent">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#Version"/>
	<rdfs:range rdf:resource="http://purl.org/net/semversion#RDFModel"/>
	<owl:maxCardinality>1</owl:maxCardinality>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasFirstParent">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:comment>A version has two parents only when merged</rdfs:comment>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#Version"/>
	<rdfs:range rdf:resource="http://purl.org/net/semversion#Version"/>
	<owl:maxCardinality>1</owl:maxCardinality>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasChild">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:comment>inverse of firstParent and secondParent, relation maintained manually</rdfs:comment>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#Version"/>
	<rdfs:range rdf:resource="http://purl.org/net/semversion#Version"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasSecondParent">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#Version"/>
	<rdfs:range rdf:resource="http://purl.org/net/semversion#Version"/>
	<owl:maxCardinality>1</owl:maxCardinality>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasChangeCause">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:comment>one of 'commit', 'branch', 'merge'</rdfs:comment>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#Version"/>
	<rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
	<owl:maxCardinality>1</owl:maxCardinality>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasContainer">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#Version"/>
	<rdfs:range rdf:resource="http://purl.org/net/semversion#VersionedModel"/>
	<owl:cardinality>1</owl:cardinality>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasName">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#User"/>
	<rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
	<owl:cardinality>1</owl:cardinality>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#hasPassword">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#User"/>
	<rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
	<owl:cardinality>1</owl:cardinality>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/net/semversion#branchLabel">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdfs:domain rdf:resource="http://purl.org/net/semversion#Version"/>
	<rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
</rdf:Description>

</rdf:RDF>