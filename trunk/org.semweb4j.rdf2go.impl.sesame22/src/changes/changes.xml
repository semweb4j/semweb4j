		<release version="4.7.0" date="2008-@@@">
			<action dev="xamde" type="fix">
				Added support for SeRQL and others in RepositoryModel.
			</action>
			<action dev="xamde" type="fix" issue="RTGO-56">
				Fixed RepositoryModel.removeAll() - thank Antoni!
			</action>
			<action dev="xamde" type="fix" issue="RTGO-49">
				Antoni fixed OSGI issues
			</action>
			<action dev="amylka" type="add">
				Upgraded to Sesame 2.2.1.
			</action>
		</release>
		

		<release version="4.6.2" date="2008-05-02">
			<action dev="xamde" type="update">
				Change bundleName and bundleSymbolicName in MANIFEST
				back to "org.openrdf.rdf2go" for backwards compatibility
				with NEPOMUK project
			</action>
		</release>

		<release version="4.6.1" date="2008-04-30">
			<action dev="xamde" type="update">
				Renamed to groupId 'org.semweb4j' and artifactId
				'rdf2go.impl.sesame20' for consistency reasons and to
				make clear that FZI is now maintaining the driver.
				Package names remain unchanged.
			</action>
			<action dev="xamde" type="fix">
				Added proper OSGI MANIFEST.MF via POM
			</action>
		</release>

		<release version="4.6.0" date="2008-04-16"
			description="Improvements and bug fixes">
			<action dev="xamde" type="update">
				Upgraded to OpenRDF 2.0.1 release
			</action>
			<action dev="xamde" type="add">
				Implemented SPARQL ASK queries
			</action>
			<action dev="xamde" type="update">
				Relocated source code to
				http(s)://semweb4j.googlecode.com/svn/trunk/org.openrdf.rdf2go
			</action>
		</release>

		<release version="4.5.4" date="2008-01-29"
			description="Bugfix">
			<action dev="xamde" type="fix">
				Fixed a bug in Model/ModelSet.update() - did not use the
				correct context
			</action>
		</release>

		<release version="4.5.3" date="2008-01-21"
			description="API extension">
			<action dev="xamde" type="update">
				Upgraded to RDF2Go 4.5.3
			</action>
